#read_file
#Script Snipplet that reads a file line by line and prints it to stout

# Reading lines in /etc/fstab.

File=/etc/fstab

{ #begin of a new codeblock function
read line1
read line2
} < $File

echo "First line in $File is:"
echo "$line1"
echo
echo "Second line in $File is:"
echo "$line2"

exit 0